@using OnshoreSDAttendanceTrackerNet.Models
@model  OnshoreSDAttendanceTrackerNet.Models.UserViewModel
@{
    ViewBag.Title = "View Team Members";
}

<h2>@ViewBag.Title</h2>

<div>
    <table class="table">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Active</th>
            </tr>
        </thead>
        <tbody>
            @foreach (UserPO user in Model.ListOfUserPO)
            {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Email</td>
                <td>@user.Active</td>
            </tr>
            }
        </tbody>
    </table>
</div>